clear;
[n,p] = size(wheelsvelocity);
running_time = 73.681429;
t = 1:n;
t = (t/n) * running_time; 
x = inpvelocity.fieldright_wheels;
y = wheelsvelocity.fieldright_wheels;

plot(t,x)
hold on;
plot(t,y)

%[n,p] = size(inpvelocity);
%t = 1:n;
%t = (t/n) * running_time;  
%leftin = inpvelocity.fieldleft_wheels;
%plot(t,leftin)
%rightout = rightout*(-1);
%[n,p] = size(wheelsvelocity);
%subplot(2,1,2)
%rightout = wheelsvelocity.fieldright_wheels;
%plot(t,rightout)
%hold on;
%plot(t,leftin)